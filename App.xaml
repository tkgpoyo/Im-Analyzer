<Application x:Class="Im_Analyzer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Im_Analyzer"
             xmlns:vm="clr-namespace:Im_Analyzer.ViewModels"
             xmlns:view="clr-namespace:Im_Analyzer.Views"
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type vm:StartUpPageViewModel}">
                <view:StartUpPage />
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>
